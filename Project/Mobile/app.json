{
  "expo": {
    "name": "FitHub",
    "slug": "FitHub",
    "scheme": "fithub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "plugins": [
        [
          "react-native-msal",
          {
            "androidPackageSignatureHash": "lvGC0B4SWYU8tNPHg/bdMjQinZQ="
          }
        ]
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.FitHub"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": ["READ_EXTERNAL_STORAGE"]
      },
      "package": "com.anonymous.FitHub"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
